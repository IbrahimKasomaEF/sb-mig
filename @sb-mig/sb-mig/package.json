{
  "name": "sb-mig",
  "version": "2.9.3",
  "description": "CLI to rule the world. (and handle stuff related to Storyblok CMS)",
  "author": "Marcin Krawczyk <marckraw@icloud.com>",
  "bin": {
    "sb-mig": "./bin/run"
  },
  "bugs": "https://github.com/sb-mig/sb-mig/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/plugin-help": "^5.1.10",
    "@oclif/plugin-plugins": "^2.0.11",
    "@oclif/plugin-warn-if-update-available": "^2.0.2",
    "@oclif/test": "^2.0.3",
    "axios": "^0.24.0",
    "camelcase": "^6.2.0",
    "dotenv": "^8.2.0",
    "glob": "^7.1.6",
    "ncp": "^2.0.0",
    "rimraf": "^3.0.2",
    "storyblok-js-client": "^4.1.5"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "conventional-changelog-cli": "^2.0.31",
    "eslint-config-oclif": "^4.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://sb-mig.com",
  "keywords": [
    "cli",
    "storyblok",
    "sb",
    "migrate"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sb-mig",
    "scope": "sb-mig",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-warn-if-update-available"
    ],
    "warn-if-update-available": {
      "timeoutInDays": 3,
      "message": "<%= config.name %> update available from <%= chalk.greenBright(config.version) %> to <%= chalk.greenBright(latest) %>."
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sb-mig/sb-mig.git"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "build": "rm -rf lib && tsc -b",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "conventional-changelog -p CHANGELOG.md -s && oclif-dev readme && git add README.md && git add CHANGELOG.md"
  },
  "types": "lib/index.d.ts"
}
