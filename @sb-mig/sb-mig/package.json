{
  "name": "sb-mig",
  "version": "3.1.10",
  "description": "CLI to rule the world. (and handle stuff related to Storyblok CMS)",
  "author": "Marcin Krawczyk <marckraw@icloud.com>",
  "license": "MIT",
  "bin": "./dist/index.js",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sb-mig/sb-mig.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "bugs": "https://github.com/sb-mig/sb-mig/issues",
  "homepage": "https://sb-mig.com",
  "keywords": [
    "cli",
    "storyblok",
    "sb",
    "migrate"
  ],
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./dist/*": "./dist/*"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc",
    "build:dev": "rm -rf dist && tsc && chmod +x ./dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "yarn build && ./dist/index.js",
    "debug": "yarn build && ./dist/index.js sync components accordion accordion-item",
    "version": "conventional-changelog -p eslint -i CHANGELOG.md -s"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "chalk": "^5.0.0",
    "dotenv": "^14.3.1",
    "glob": "^7.2.0",
    "meow": "^10.1.2",
    "ncp": "^2.0.0",
    "storyblok-js-client": "^4.2.0"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^2.0.0",
    "@types/glob": "^7.2.0",
    "@types/ncp": "^2.0.5",
    "@types/node": "^17.0.35",
    "conventional-changelog-cli": "^2.0.31",
    "typescript": "^4.6.4"
  }
}
