{
  "name": "@sb-mig/plugin-generate-project",
  "private": false,
  "description": "Plugin for sb-mig to generate Storyblok project with command line",
  "version": "0.4.1",
  "author": "Marcin Krawczyk marckraw@icloud.com",
  "bugs": "https://github.com/sb-mig/plugin-generate-project/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@sb-mig/plugin-add-components": "^0.5.1",
    "@types/rimraf": "^3.0.0",
    "execa": "^4.0.0",
    "ncp": "^2.0.0",
    "ora": "^4.0.4",
    "os": "^0.1.1",
    "rimraf": "^3.0.2",
    "sb-mig": "^2.8.1",
    "tslib": "^1",
    "yarn-or-npm": "^3.0.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/yarn.lock"
  ],
  "homepage": "https://github.com/sb-mig/plugin-generate-project",
  "keywords": [
    "generate-project",
    "sb-mig",
    "storyblok",
    "plugin",
    "generate"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sb-mig",
    "devPlugins": [
      "@oclif/plugin-help",
      "@sb-mig/plugin-add-components"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sb-mig/plugin-generate-project.git"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "build": "rm -rf lib && tsc -b",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
