{
  "name": "@sb-mig/plugin-generate-project",
  "private": false,
  "description": "Plugin for sb-mig to generate Storyblok project with command line",
  "version": "0.4.12",
  "author": "Marcin Krawczyk marckraw@icloud.com",
  "bugs": "https://github.com/sb-mig/sb-mig/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@sb-mig/plugin-add-components": "^0.5.12",
    "ncp": "^2.0.0",
    "rimraf": "^3.0.2",
    "sb-mig": "^2.9.3",
    "yarn-or-npm": "^3.0.1"
  },
  "devDependencies": {
    "eslint-config-oclif": "^3.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/yarn.lock"
  ],
  "homepage": "https://github.com/sb-mig/sb-mig",
  "keywords": [
    "generate-project",
    "sb-mig",
    "storyblok",
    "plugin",
    "generate"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sb-mig",
    "devPlugins": [
      "@oclif/plugin-help",
      "@sb-mig/plugin-add-components"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sb-mig/sb-mig.git"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "build": "rm -rf lib && tsc -b",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "conventional-changelog -p eslint -i CHANGELOG.md -s"
  }
}
