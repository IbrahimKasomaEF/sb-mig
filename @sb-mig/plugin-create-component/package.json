{
  "name": "@sb-mig/plugin-create-component",
  "description": "Plugin for sb-mig to create component in monorepos compliant with sb-mig",
  "version": "0.1.25",
  "author": "Marcin Krawczyk marckraw@icloud.com",
  "bugs": "https://github.com/sb-mig/sb-mig/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "rimraf": "^3.0.2",
    "sb-mig": "^2.9.11",
    "yarn-or-npm": "^3.0.1"
  },
  "devDependencies": {
    "conventional-changelog-cli": "^2.0.31",
    "eslint-config-oclif": "^3.1"
  },
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "files": [
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/yarn.lock"
  ],
  "homepage": "https://github.com/sb-mig/sb-mig",
  "keywords": [
    "create-component",
    "sb-mig",
    "storyblok",
    "plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "scope": "sb-mig",
    "bin": "sb-mig",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sb-mig/sb-mig.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && npx oclif-dev manifest && npx oclif-dev readme",
    "build": "rm -rf lib && tsc -b",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "conventional-changelog -p eslint -i CHANGELOG.md -s"
  }
}
