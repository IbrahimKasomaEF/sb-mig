{
  "name": "sb-mig",
  "version": "4.1.0-beta.53",
  "description": "CLI to rule the world. (and handle stuff related to Storyblok CMS)",
  "author": "Marcin Krawczyk <marckraw@icloud.com>",
  "license": "MIT",
  "bin": "./dist/index.js",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sb-mig/sb-mig.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "bugs": "https://github.com/sb-mig/sb-mig/issues",
  "homepage": "https://github.com/sb-mig/sb-mig",
  "keywords": [
    "cli",
    "storyblok",
    "sb",
    "migrate"
  ],
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./dist/*": "./dist/*"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "files": [
    "/dist",
    "README.md"
  ],
  "scripts": {
    "lint-staged": "lint-staged",
    "build": "rm -rf dist && tsc -p tsconfig.json && chmod +x ./dist/index.js",
    "build:dev": "chokidar 'src/**/*.{js,ts,cjs,mjs}' -c 'yarn build'",
    "build:test": "tsc -p tsconfig.test.json",
    "build:test:dev": "chokidar '__tests__/**/*.ts' -c 'yarn build:test'",
    "test": "mocha ./dist-tests/__tests__ --require esm",
    "start": "yarn build && ./dist/index.js",
    "debug": "yarn build && ./dist/index.js sync components accordion accordion-item",
    "version": "conventional-changelog -p eslint -i CHANGELOG.md -s",
    "semantic-release": "semantic-release",
    "prepare": "husky install"
  },
  "dependencies": {
    "@swc/core": "^1.3.38",
    "@swc/helpers": "^0.4.14",
    "chalk": "^5.1.2",
    "dotenv": "^16.0.3",
    "fs-extra": "^11.1.0",
    "glob": "8",
    "meow": "^11.0.0",
    "ncp": "^2.0.0",
    "rollup": "^3.18.0",
    "rollup-plugin-ts": "^3.2.0",
    "storyblok-js-client": "^5.6.0",
    "typescript": "^4.9.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@ryansonshine/commitizen": "^4.2.8",
    "@ryansonshine/cz-conventional-changelog": "^3.3.4",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@sindresorhus/tsconfig": "^3.0.1",
    "@types/chai": "^4.3.4",
    "@types/fs-extra": "^11.0.1",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/ncp": "^2.0.5",
    "@types/node": "^18.14.6",
    "@types/sinon": "^10.0.13",
    "@types/uuid": "^9.0.1",
    "chai": "^4.3.7",
    "chokidar-cli": "^3.0.0",
    "conventional-changelog-cli": "^2.0.31",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "esm": "^3.2.25",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "mocha": "^10.2.0",
    "prettier": "^2.8.4",
    "semantic-release": "^20.1.1",
    "sinon": "^15.0.1",
    "tsc-files": "^1.1.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@ryansonshine/cz-conventional-changelog"
    }
  }
}
