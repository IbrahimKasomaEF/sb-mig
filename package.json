{
  "name": "sb-mig",
  "homepage": "https://github.com/marckraw/sb-mig.git",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marckraw/sb-mig.git"
  },
  "version": "1.1.3",
  "description": "CLI to rule the world. (and handle stuff related to Storyblok CMS)",
  "keywords": [
    "cli",
    "storyblok",
    "sb",
    "migrate"
  ],
  "main": "./dist/index.js",
  "scripts": {
    "test": "jest",
    "start": "node index.js",
    "build": "gulp compress",
    "prepare": "npm run test && npm run build",
    "preversion": "npm run prepare",
    "prepublish:patch": "npm run prepare",
    "prepublish:minor": "npm run prepare",
    "prepublish:major": "npm run prepare",
    "publish:patch": "npm version patch && npm publish --dry-run",
    "publish:minor": "npm version minor",
    "publish:major": "npm version major",
    "version": "conventional-changelog -p eslint -i CHANGELOG.md -s && git add CHANGELOG.md",
    "prettier": "prettier  --write \"./src/**/*.js\" --config .prettierrc"
  },
  "author": "Marcin Krawczyk <marckraw@icloud.com>",
  "license": "MIT",
  "dependencies": {
    "chalk": "^3.0.0",
    "commander": "^4.0.1",
    "dotenv": "^8.2.0",
    "figlet": "^1.2.4",
    "glob": "^7.1.6",
    "node-fetch": "^2.6.0",
    "storyblok-js-client": "^2.0.12"
  },
  "devDependencies": {
    "conventional-changelog-cli": "^2.0.31",
    "gulp": "^4.0.2",
    "gulp-uglify": "^3.0.2",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "pump": "^3.0.0",
    "uglify-es": "^3.3.9"
  },
  "bin": {
    "sb-mig": "./dist/index.js"
  },
  "engines": {
    "node": ">=11"
  }
}
