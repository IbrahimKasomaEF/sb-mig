{
  "name": "sb-mig",
  "version": "2.1.5",
  "description": "CLI to rule the world. (and handle stuff related to Storyblok CMS)",
  "author": "Marcin Krawczyk <marckraw@icloud.com>",
  "bin": {
    "sb-mig": "./bin/run"
  },
  "bugs": "https://github.com/sb-mig/sb-mig/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/plugin-plugins": "^1.7.10",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "@types/ncp": "^2.0.3",
    "camelcase": "^5.3.1",
    "dotenv": "^8.2.0",
    "execa": "^4.0.0",
    "figlet": "^1.3.0",
    "glob": "^7.1.6",
    "ncp": "^2.0.0",
    "ora": "^4.0.4",
    "rimraf": "^3.0.2",
    "storyblok-js-client": "^2.4.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/figlet": "^1.2.0",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "conventional-changelog-cli": "^2.0.31",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://helper.sb-mig.com",
  "keywords": [
    "cli",
    "storyblok",
    "sb",
    "migrate"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sb-mig",
    "scope": "sb-mig",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-warn-if-update-available"
    ],
    "warn-if-update-available": {
      "timeoutInDays": 3,
      "message": "<%= config.name %> update available from <%= chalk.greenBright(config.version) %> to <%= chalk.greenBright(latest) %>."
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sb-mig/sb-mig.git"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "conventional-changelog -p eslint -i CHANGELOG.md -s && oclif-dev readme && git add README.md && git add CHANGELOG.md"
  },
  "types": "lib/index.d.ts"
}
