{
  "name": "sb-mig",
  "version": "4.1.0-beta.71",
  "description": "CLI to rule the world. (and handle stuff related to Storyblok CMS)",
  "author": "Marcin Krawczyk <marckraw@icloud.com>",
  "license": "MIT",
  "bin": "./dist/index.js",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sb-mig/sb-mig.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "bugs": "https://github.com/sb-mig/sb-mig/issues",
  "homepage": "https://github.com/sb-mig/sb-mig",
  "keywords": [
    "cli",
    "storyblok",
    "sb",
    "migrate"
  ],
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./dist/*": "./dist/*"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "files": [
    "/dist",
    "README.md"
  ],
  "scripts": {
    "lint-staged": "lint-staged",
    "build": "rm -rf dist && tsc -p tsconfig.json && chmod +x ./dist/index.js",
    "build:dev": "chokidar 'src/**/*.{js,ts,cjs,mjs}' -c 'yarn build'",
    "build:test": "tsc -p tsconfig.test.json",
    "build:test:dev": "chokidar '__tests__/**/*.ts' -c 'yarn build:test'",
    "lint:fix": "eslint --fix",
    "lint:all": "eslint .",
    "lint:fix:all": "eslint . --fix",
    "lint:ts": "cd src && tsc --declaration --emitDeclarationOnly && cd ..",
    "test": "mocha ./dist-tests/__tests__ --require esm",
    "start": "yarn build && ./dist/index.js",
    "debug": "yarn build && ./dist/index.js sync components accordion accordion-item",
    "version": "conventional-changelog -p eslint -i CHANGELOG.md -s",
    "semantic-release": "semantic-release",
    "prepare": "husky install"
  },
  "dependencies": {
    "@swc/core": "1.3.41",
    "@swc/helpers": "0.4.14",
    "chalk": "^5.1.2",
    "dotenv": "^16.0.3",
    "form-data": "^4.0.0",
    "fs-extra": "^11.1.1",
    "glob": "8",
    "meow": "^11.0.0",
    "ncp": "^2.0.0",
    "node-fetch": "^3.3.1",
    "rollup": "^3.20.2",
    "rollup-plugin-ts": "^3.2.0",
    "storyblok-js-client": "^5.10.2",
    "typescript": "^5.0.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@ryansonshine/commitizen": "^4.2.8",
    "@ryansonshine/cz-conventional-changelog": "^3.3.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@sindresorhus/tsconfig": "^3.0.1",
    "@types/chai": "^4.3.4",
    "@types/fs-extra": "^11.0.1",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/ncp": "^2.0.5",
    "@types/node": "^18.15.11",
    "@types/sinon": "^10.0.13",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "chai": "^4.3.7",
    "chokidar-cli": "^3.0.0",
    "conventional-changelog-cli": "^2.0.31",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "esm": "^3.2.25",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "mocha": "^10.2.0",
    "prettier": "^2.8.7",
    "semantic-release": "^21.0.1",
    "sinon": "^15.0.3",
    "tsc-files": "^1.1.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@ryansonshine/cz-conventional-changelog"
    }
  }
}
